<div class="container">
  <%= @restaurant.name %>
  <br>
  <%= @restaurant.address %>
  <br>
  <%= @restaurant.phone_number %>
  <br>
  <%= @restaurant.category %>
  <br>
  <%  @restaurant.reviews.each do |review|%>
  <%= review.rating %>
  <%= review.content %>
  <% end %>

  <br>
  <%= link_to 'Back', restaurants_path %> |
  <%= link_to 'Leave a review', new_restaurant_review_path(@restaurant) %>
</div>

